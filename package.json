{
  "name": "myapp",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.tsx",
  "scripts": {
    "start": "webpack-dev-server -d --config=./config/webpack.development.config.js --hot --open",
    "build:dev": "webpack --config=./config/webpack.development.config.js --mode=development",
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "release": "node ./bin/release.js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.1.3",
    "date-fns": "1.29.0",
    "font-awesome": "4.7.0",
    "immutable": "3.8.2",
    "react": "^16.4.2",
    "react-color": "2.14.1",
    "react-dom": "^16.4.2",
    "react-fa": "5.0.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "reactstrap": "^6.4.0",
    "styled-components": "3.4.2",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@types/immutable": "3.8.7",
    "@types/jest": "23.3.1",
    "@types/react": "^16.4.8",
    "@types/react-color": "2.13.6",
    "@types/react-dom": "^16.0.7",
    "@types/react-fa": "4.1.5",
    "@types/react-router": "4.0.30",
    "@types/react-router-dom": "4.3.0",
    "@types/reactstrap": "6.0.2",
    "@types/webpack": "4.4.9",
    "@types/webpack-env": "1.13.6",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "file-loader": "1.1.11",
    "fork-ts-checker-webpack-plugin": "0.4.4",
    "hard-source-webpack-plugin": "0.11.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.4.2",
    "jest-bamboo-formatter": "1.0.1",
    "lint-staged": "7.2.0",
    "node-sass": "4.9.2",
    "path": "0.12.7",
    "prettier": "1.14.0",
    "react-testing-library": "3.1.7",
    "sass-loader": "7.1.0",
    "sass-variable-loader": "0.1.2",
    "style-loader": "0.22.0",
    "ts-jest": "23.1.3",
    "ts-loader": "4.4.2",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.0.1",
    "url-loader": "1.0.1",
    "url-search-params-polyfill": "4.0.1",
    "webpack": "4.16.5",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "worker-loader": "2.0.0"
  },
  "config": {
    "msvs_version": "2015"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.(js|jsx|ts|tsx)",
      "!**/node_modules/**",
      "!**/definitions/**"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "testResultsProcessor": "<rootDir>/node_modules/jest-bamboo-formatter",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/reports/",
      "<rootDir>/public/",
      "<rootDir>/config/",
      "<rootDir>/definitions/",
      "<rootDir>/setup/",
      "<rootDir>/src/Polyfills",
      "index.(t|j)s"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg)$": "identity-obj-proxy",
      "^Utilities/(.*)": "<rootDir>/src/App/Utilities/$1",
      "^Components/(.*)": "<rootDir>/src/App/Components/$1",
      "^App/(.*)": "<rootDir>/src/App/$1",
      "^Src/(.*)": "<rootDir>/src/$1",
      "^Pages/(.*)": "<rootDir>/src/App/Pages/$1",
      "^API/(.*)": "<rootDir>/src/App/API/$1",
      "^Enums/(.*)": "<rootDir>/src/App/Enums/$1"
    },
    "testRegex": "\\.test\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "jsx",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/setup/Tests/shim.js"
    ],
    "watchman": false
  }
}
